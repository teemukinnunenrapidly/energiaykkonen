{
  "name": "energiaykkonen-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "test:email": "node scripts/validate-email-system.js",
    "test:email:full": "node scripts/validate-email-system.js --mode full",
    "build:widget": "webpack --config webpack.widget.config.js --mode production",
    "build:widget:dev": "webpack --config webpack.widget.config.js --mode development",
    "watch:widget": "webpack --config webpack.widget.config.js --watch --mode development",
    "deploy:wordpress": "npm run build:widget && node scripts/deploy-to-wordpress.js",
    "test:widget": "npm run build:widget:dev && serve dist",
    "test:isolation": "npm run build:widget && npm run test:browsers",
    "test:css-outputs": "npm run build:widget && node scripts/verify-css-outputs.js",
    "verify:build": "npm run build:widget && npm run test:css-outputs",
    "cleanup:old": "node scripts/cleanup-old-files.js",
    "test:browsers": "node scripts/browser-testing.js",
    "test:browsers:chrome": "node scripts/browser-testing.js --browser=chrome",
    "test:browsers:firefox": "node scripts/browser-testing.js --browser=firefox",
    "test:browsers:safari": "node scripts/browser-testing.js --browser=safari",
    "test:browsers:edge": "node scripts/browser-testing.js --browser=edge",
    "test:browsers:ie11": "node scripts/browser-testing.js --browser=ie11",
    "test:manual": "npm run build:widget && node scripts/browser-testing.js --generate-pages && open test-pages/shadow-dom-test.html",
    "test:wordpress": "node scripts/test-wordpress-integration.js",
    "test:performance": "node scripts/performance-profiler.js",
    "test:css-isolation": "node scripts/css-isolation-tester.js",
    "test:error-handling": "node scripts/error-scenario-tester.js",
    "test:error-unit": "jest src/components/error-handling/__tests__/",
    "test:error-all": "npm run test:error-unit && npm run build:widget && npm run test:error-handling"
  },
